<?xml version="1.0" encoding="utf-8"?>
<s:View xmlns:fx="http://ns.adobe.com/mxml/2009"
		xmlns:s="library://ns.adobe.com/flex/spark"
		creationComplete="viewActivateHandler(event)" title="Video Player View">
	<fx:Script>
		<![CDATA[
			import mx.events.FlexEvent;
			import player.OSMFPlayer;
			
			private var osmfplayer:OSMFPlayer;
			
			protected function viewActivateHandler(event:Event):void
			{
				osmfplayer = new OSMFPlayer();
				this.addElement(osmfplayer);
			}
			
		]]>
	</fx:Script>
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
</s:View>
